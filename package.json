{
  "name": "leave-the-nest",
  "version": "0.1.0",
  "private": true,
  "scripts": {
    "dev": "next dev",
    "build": "compile:fr && next build",
    "start": "next start -p $PORT",
    "lint": "next lint",
    "format:staged": "pretty-quick --staged",
    "format": "prettier --write pages",
    "extract": "formatjs extract",
    "compile": "formatjs compile",
    "compile:fr": "npm run compile -- lang/fr.json --ast --out-file compiled-lang/fr.json"
  },
  "prettier": {
    "semi": false
  },
  "dependencies": {
    "@next-auth/mongodb-adapter": "^1.1.1",
    "@types/node": "18.11.9",
    "@types/react": "18.0.25",
    "@types/react-dom": "18.0.9",
    "bluebird": "^3.7.2",
    "eslint": "8.28.0",
    "eslint-config-next": "13.0.4",
    "husky": "^8.0.2",
    "mongodb": "^4.12.0",
    "next": "13.0.4",
    "next-auth": "^4.16.4",
    "parse-link-header": "^2.0.0",
    "prettier": "^2.7.1",
    "pretty-quick": "^3.1.3",
    "react": "18.2.0",
    "react-dom": "18.2.0",
    "react-intl": "^6.2.1",
    "typescript": "4.9.3"
  },
  "devDependencies": {
    "@formatjs/cli": "^5.1.3"
  }
}
